! General
URxvt.termName          : rxvt-unicode-256color
URxvt.depth             : 32
URxvt.geometry          : 90x30
URxvt.transparent       : true
URxvt.shading           : 7
URxvt.fading            : 40
URxvt.loginShell        : true
URxvt.saveLines         : 0
URxvt.scrollBar         : false
URxvt.scrollTtyOutput   : false
URxvt.scrollWithBuffer  : true
URxvt.scrollTtyKeypress : true
URxvt.scrollStyle       : rxvt
URxvt.secondaryScreen   : 1
URxvt.secondaryScroll   : 0
URxvt.internalBorder    : 3
URxvt.lineSpace         : 0
!
URxvt.iso14755          : false
URxvt.iso14755_52       : false
!
! Extensions
URxvt.perl-lib        : /usr/lib/urxvt/perl/
URxvt.perl-ext-common : default,searchable-scrollback,url-select,clipboard
URxvt.matcher.button  : 1
!  - catch ugly URLs
!  - url-select
URxvt.keysym.M-u           : perl:url-select:select_next
URxvt.url-select.launcher  : google-chrome-unstable
URxvt.url-select.underline : true
!  - clipboard
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

! Appearance
URxvt*font: xft:Source\ Code\ Pro:pixelsize=13
URxvt*boldFont: xft:Source\ Code\ Pro:pixelsize=13
!   - Fix font space
URxvt*letterSpace: -1
!
! ----[ Xft settings ]----
!
Xft.dpi       : 96
Xft.antialias : true
Xft.rgba      : rgb
Xft.hinting   : true
Xft.hintstyle : hintslight
!
URxvt.intensityStyles: false

! Solarized Light
